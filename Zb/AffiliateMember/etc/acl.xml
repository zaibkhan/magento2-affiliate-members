<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Zb_AffiliateMember::affiliatemember" title="Affiliate Members" sortOrder="60">
                    <resource id="Zb_AffiliateMember::affiliatemember_affiliatemember" title="Affiliate members" sortOrder="10"/>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Zb_AffiliateMember::affiliatemember_config" title="Affiliate Members"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
